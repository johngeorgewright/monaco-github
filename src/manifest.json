{
  "name": "Monaco GitHub Editor",
  "description": "Use Monaco as your GitHub editor",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "declarativeContent",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "editor.html"
  }
}